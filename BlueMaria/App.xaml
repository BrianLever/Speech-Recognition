            <!--
        we dont' need 2 styles here (for moues over), just bind to templated parent and use background, foreground colors
        -->
            <Style x:Key="ButtonStyleClose" TargetType="{x:Type Button}">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <Viewbox Stretch="Uniform">

                                <Canvas Width="18" Height="18" x:Name="ParentCanvas">

                                    <Grid Width="{Binding ElementName=ParentCanvas, Path=ActualWidth}" Height="{Binding ElementName=ParentCanvas, Path=ActualHeight}">

                                        <!--Fill="#FFFDFDFD"-->
                                        <Ellipse Width="18" Height="18" Fill="{TemplateBinding Background}" StrokeThickness="1" Stroke="#FFFDFDFD" Opacity="1"/>

                                        <Rectangle Width="15" Height="2" Fill="#FF383838" Opacity="1" RenderTransformOrigin="0.5,0.5">
                                            <Rectangle.RenderTransform>
                                                <RotateTransform Angle="-45"/>
                                            </Rectangle.RenderTransform>
                                        </Rectangle>

                                        <Rectangle Width="15" Height="2" Fill="#FF383838" Opacity="1" RenderTransformOrigin="0.5,0.5">
                                            <Rectangle.RenderTransform>
                                                <RotateTransform Angle="45"/>
                                            </Rectangle.RenderTransform>
                                        </Rectangle>
                                    </Grid>
                                </Canvas>
                            </Viewbox>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
                <Setter Property="BorderBrush" Value="{x:Null}"/>
                <Setter Property="Foreground" Value="{x:Null}"/>
                <Setter Property="Background" Value="{x:Null}"/>
            </Style>

